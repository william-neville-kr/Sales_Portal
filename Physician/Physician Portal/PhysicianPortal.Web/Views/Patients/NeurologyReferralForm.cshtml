@using PhysicianPortal.Web.Models
@model RefFormNeurologyViewModel
@{
    ViewBag.Title = "Neurology Referral Form";
    Layout = null;
}

@using (Ajax.BeginForm("NeurologyReferralForm", "Patients", new AjaxOptions { OnSuccess = "OnSuccessReferralForm", OnFailure = "OnFailureReferralForm" }, new { @class = "ReferralForm display-inline-block form-style", @id = "NeurologyForm" }))
