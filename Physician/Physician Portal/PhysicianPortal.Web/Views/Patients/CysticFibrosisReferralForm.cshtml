@using PhysicianPortal.Web.Models
@model RefFormCysticFibrosisViewModel
@{
    ViewBag.Title = "Cystic Fibrosis Referral Form";
    Layout = null;
}

@using (Ajax.BeginForm("CysticFibrosisReferralForm", "Patients", new AjaxOptions { OnSuccess = "OnSuccessReferralForm", OnFailure = "OnFailureReferralForm" }, new { @class = "ReferralForm display-inline-block form-style", @id = "CysticFibrosisForm" }))
